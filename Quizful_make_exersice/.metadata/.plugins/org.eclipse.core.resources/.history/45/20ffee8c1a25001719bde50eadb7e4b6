package max_min_avg;

public class Main {

	public static void main(String[] args) {
		
		int mas[]=new int[5];
mas[0]=1;
mas[1]=2;
mas[2]=3;
mas[3]=4;
mas[4]=5;
		
		int max =0;
		for (int i = 0; i < mas.length; i++) {
			if (mas[i]<mas[i+1]){
				max = mas[i];
			}
			System.out.println("max = " + max);
		}
	
		
		
	}
}
