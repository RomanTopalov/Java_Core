package max_min_avg;

public class Main {

	public static void main(String[] args) {
		
		int mas[]={10,2,3,4,5};
		int max =0;
	
		for (int i = 0; i < mas.length; i++) {
			if (mas[i]>mas[i+1]){
				max = mas[i];
			
			}else {
				max = mas[i+1];
			
			}
			System.out.println("max = " + max);
		}
	
		
	}
}
